   "ErrorFunction": "DEMO",
   "Events": {
      "001": [
         "// Set the horizontal slider min max values.",
         "#Min = 1",
         "#Max = 9",
         "",
         "$SliderVertical = 'Mar'",
         "",
         "//Set the possible values for the vertical slider",
         "$Month(1)  = 'Jan'",
         "$Month(2)  = 'Feb'",
         "$Month(3)  = 'Mar'",
         "$Month(4)  = 'Apr'",
         "$Month(5)  = 'May'",
         "$Month(6)  = 'Jun'",
         "$Month(7)  = 'Jul'",
         "$Month(8)  = 'Aug'",
         "$Month(9)  = 'Sep'",
         "$Month(10) = 'Oct'",
         "$Month(11) = 'Nov'",
         "$Month(12) = 'Dec'"
      ],
      "002": [
         "//Show value selected for horizontal slider.",
         "ValueSelectedHorizontal.Label = conv(#INPUT)",
         "",
         "//Set the property for the vertical slider",
         "VerticalSlider.Axis_tickMarks_interval = #INPUT"
      ],
      "003": [
         "//Retrieve current value of displayed vertical slider properties.",
         "$BackColor        = VerticalSlider.Background_fill_color",
         "#TickMarkInterval = VerticalSlider.Axis_tickMarks_interval",
         "#Increment        = VerticalSlider.SingleIncrement",
         "#PageIncrement    = VerticalSlider.PageIncrement",
         "$ValueSelectedHorizontal = CONV(#TickMarkInterval)",
         "",
         "if VerticalSlider.ShowValue # 0 then",
         "   $ShowValue = 'Y'",
         "endif",
         "",
         "$ValueSelectedVertical   = $Month(1)"
      ],
      "004": [
         "//Exit to DEMO",
         "lsexit"
      ],
      "005": [
         "//Reset the vertical slider values to the defaults.",
         "#Increment = 1",
         "#PageIncrement = 3",
         "",
         "VerticalSlider.Background_fill_color = 'Aqua'",
         "$BackColor = VerticalSlider.Background_fill_color",
         "VerticalSlider.ShowValue = True",
         "$ShowValue = 'Y'",
         "VerticalSlider.Axis_tickMarks_interval = 2",
         "#TickMarkInterval = VerticalSlider.Axis_tickMarks_interval"
      ],
      "006": [
         "//Set the new property values.",
         "VerticalSlider.Background_fill_color = $INPUT"
      ],
      "007": [
         "//Show value selected for vertical slider.",
         "ValueSelectedVertical.Label = $INPUT"
      ],
      "010": [
         "//Refresh values and the slider itself.",
         "VerticalSlider.SingleIncrement = #INPUT"
      ],
      "011": [
         "//Refresh values and the slider itself.",
         "VerticalSlider.PageIncrement = #INPUT"
      ],
      "012": [
         "//Set the property according to the variable value.",
         "if $INPUT = 'Y' then",
         "   VerticalSlider.ShowValue = True",
         "else",
         "   VerticalSlider.ShowValue = False",
         "endif"
      ]
   },
