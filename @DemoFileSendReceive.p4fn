         "//",
         "// Add trailing slash to directory if missing",
         "//",
         ":1",
         "$OLD_HOST_DIR = $HOST_DIR",
         "#LEN = LEN($HOST_DIR)",
         "$SLASH = $HOST_DIR(#LEN,#LEN)",
         "CASE @SYSID",
         "   WHEN 'MSDOS' : IF $SLASH # '\\'",
         "                     $HOST_DIR = $HOST_DIR + '\\'",
         "                  ENDIF",
         "   WHEN 'UNIX' : IF $SLASH # '/'",
         "                     $HOST_DIR = $HOST_DIR + '/'",
         "                  ENDIF",
         "ENDCASE",
         "//",
         "$FORMATED = ''",
         "//",
         "WHILE $FORMATED = ''",
         "   #QUOTE_POS = INDEX($DIR,'\"')",
         "   IF #QUOTE_POS > 0 THEN",
         "      $DIR = $DIR(1,#QUOTE_POS - 1) + $DIR(#QUOTE_POS + 1,LEN($DIR))",
         "      $QUOTES = '\"'",
         "   ELSE",
         "      $FORMATED = 'Y'",
         "   ENDIF",
         "ENDWHILE",
         "//",
         "#LEN = LEN($DIR)",
         "$SLASH = $DIR(#LEN,#LEN)",
         "IF $SLASH # '\\' THEN",
         "   $DIR = $DIR + '\\'",
         "ENDIF",
         "RETURN",
         "//",
