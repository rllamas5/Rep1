         "   $INVALID_HOST_DIR = 'Y'",
         "   UMSG('Directory listing failed error code ' + PIC(#RC,'99'),-1)",
         "ELSE",
         "   FLD(\"HostFile\")",
         "ENDIF",
         "RETURN",
         ":3",
         "CALL 1",
         "$TARGET_FILE = $HOST_DIR + $CLIENT_FILE",
         "$SOURCE_FILE = $DIR + $CLIENT_FILE",
         "#RC = CLIENT.Receive($TARGET_FILE,$SOURCE_FILE)",
         "IF #RC # 0 THEN",
         "   UMSG('Receive file failed error code ' + PIC(#RC,'99'),-1)",
         "   $INVALID_HOST_DIR = 'Y'",
         "ELSE",
         "   $HOST_FILE = $CLIENT_FILE",
         "   CALL 2",
         "ENDIF",
         "RETURN"
      ],
