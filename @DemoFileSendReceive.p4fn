   "Events": {
      "001": [
         "$OLD_CLIENT_DIR = $DIR",
         "$SEARCH_DIR = $DIR",
         "CALL 1",
         "$INVALID_CLIENT_DIR = ''",
         "#RC = SYSTEM('GUI DIR /B /ON \"' + $SEARCH_DIR + '*.*\" > CDIR.P4X')",
         "IF #RC > 0",
         "   UMSG('DIR command failed error code ' + PIC(#RC,'99'),-1)",
         "   $INVALID_CLIENT_DIR = 'Y'",
         "ELSE",
         "   #RC = CLIENT.Receive($PRODATA + 'dmtxcx.txt','CDIR.P4X')",
         "   IF #RC > 0",
         "      UMSG('Receive file failed error code ' + PIC(#RC,'99'),-1)",
         "   ELSE",
         "      FLD(\"ClientFile\")",
         "   ENDIF",
         "ENDIF",
         "",
         ":1",
         "#LEN = LEN($SEARCH_DIR)",
         "$SLASH = $SEARCH_DIR(#LEN,#LEN)",
         "IF $SLASH # '\\' THEN",
         "   $SEARCH_DIR = $SEARCH_DIR + '\\'",
         "ENDIF",
         "RETURN"
      ],
      "002": [
         "// Add trailing slash to directory if missing",
         "$OLD_HOST_DIR = $HOST_DIR",
         "$SEARCH_DIR = $HOST_DIR",
         "CALL 1",
         "HostDirectory.Refresh()",
         "// list directory to file on host system",
         "$INVALID_HOST_DIR = ''",
         "CASE @SYSID",
         "   WHEN 'UNIX' :",
         "      #RC = SYSTEM('ls -1 \"' + $SEARCH_DIR + '\" > \"' + $PRODATA + 'dmtxrx.txt\"')",
         "   WHEN 'MSDOS' :",
         "      #RC = SYSTEM('DIR /B /ON \"' + $SEARCH_DIR + '\" > \"' + $PRODATA + 'dmtxrx.txt\"')",
         "   WHEN 'VMS' :",
         "      #RC = SYSTEM('DIR ' + $SEARCH_DIR + ' OUTPUT=' + $PRODATA + 'dmtxrx.txt')",
         "ENDCASE",
         "IF #RC # 0 THEN",
         "   $INVALID_HOST_DIR = 'Y'",
         "   UMSG('Directory listing failed error code ' + PIC(#RC,'99'),-1)",
         "ELSE",
         "   HostFile.Refresh()",
         "ENDIF",
         "",
         ":1",
         "#LEN = LEN($SEARCH_DIR)",
         "$SLASH = $SEARCH_DIR(#LEN,#LEN)",
         "CASE @SYSID",
         "   WHEN 'MSDOS' : IF $SLASH # '\\'",
         "                     $SEARCH_DIR = $SEARCH_DIR + '\\'",
         "                  ENDIF",
         "   WHEN 'UNIX' : IF $SLASH # '/'",
         "                     $SEARCH_DIR = $SEARCH_DIR + '/'",
         "                  ENDIF",
         "ENDCASE",
         "RETURN"
      ],
      "003": ["FNEXIT"],
      "004": [
         "$DIR = 'C:\\'",
         "$HOST_DIR = &$@~PRODATA",
         "IF $HOST_DIR = '' THEN",
         "   UMSG('The PROIV system variable PRODATA must be defined (in pro4.ini or as an environment ' +",
         "   'variable)',-1)",
         "   FNEXIT",
         "ENDIF",
         "//",
         "#LEN = LEN($HOST_DIR)",
         "$SLASH = $HOST_DIR(#LEN,#LEN)",
         "CASE @SYSID",
         "   WHEN 'MSDOS' : IF $SLASH # '\\' THEN",
         "                     $HOST_DIR = $HOST_DIR + '\\'",
         "                  ENDIF",
         "   WHEN 'UNIX' : IF $SLASH # '/' THEN",
         "                     $HOST_DIR = $HOST_DIR + '/'",
         "                  ENDIF",
         "ENDCASE",
         "//",
         "// save $host_dir at this point to $PRODATA as this will be where the listings of the current",
         "// directory will be sent to",
         "//",
         "$PRODATA = $HOST_DIR",
         "//",
         "// invoke a client side system command to list the contents of the C: directory.",
         "// Redirect the output to a file and then send it to the server so it can be",
         "// displayed in the list box.",
         "//",
         "#RC = SYSTEM('GUI DIR  /B /ON \"' + $DIR + '*.*\" > CDIR.P4X')",
         "$INVALID_CLIENT_DIR = ''",
         "IF #RC > 0 THEN",
         "   UMSG('DIR command failed error cod ' + PIC(#RC,'99'),-1)",
         "   $INVALID_CLIENT_DIR = 'Y'",
         "ELSE",
         "   #RC = CLIENT.Receive($PRODATA + 'dmtxcx.txt','CDIR.P4X')",
         "   IF #RC > 0 THEN",
         "      UMSG('Receive file failed error code ' + PIC(#RC,'99'),-1)",
         "   ENDIF",
         "ENDIF",
         "//",
         "// list directory to file on host system",
         "//",
         "$INVALID_HOST_DIR = ''",
         "CASE @SYSID",
         "   WHEN 'UNIX' :",
         "      #RC = SYSTEM('ls -1 \"' + $HOST_DIR + '\" > \"' + $PRODATA + 'dmtxrx.txt\"')",
         "   WHEN 'MSDOS' :",
         "      #RC = SYSTEM('DIR /B /ON \"' + $HOST_DIR + '\" > \"' + $PRODATA + 'dmtxrx.txt\"')",
         "   WHEN 'VMS' :",
         "      #RC = SYSTEM('DIR ' + $HOST_DIR + ' OUTPUT=' + $PRODATA + 'dmtxrx.txt')",
         "ENDCASE",
         "//",
         "IF #RC # 0 THEN",
         "   UMSG('Directory listing failed error code ' + PIC(#RC,'99'),-1)",
         "   $INVALID_HOST_DIR = 'Y'",
         "ENDIF"
      ],
      "006": [
         "IF @SYSPASS = 'Y' ELSE",
         "   IF $OLD_CLIENT_DIR = $DIR ELSE",
         "      $SEARCH_DIR = $DIR",
         "      CALL 1",
         "      $INVALID_CLIENT_DIR = ''",
         "      #RC = SYSTEM('GUI DIR  /B /ON \"' + $SEARCH_DIR + '*.*\" > CDIR.P4X')",
