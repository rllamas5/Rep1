         "            #SPACE_POS = INDEX($CLIENT_FILE,' ')",
         "            IF #SPACE_POS > 0 THEN",
         "               IF @SYSID = 'MSDOS' THEN",
         "                  CALL 3",
         "               ELSE",
         "                  MSG(5)",
         "                  FLD(\"ClientFile\")",
         "               ENDIF",
         "            ELSE",
         "               CALL 3",
         "            ENDIF",
         "         ELSE",
         "            MSG(2)",
         "            FLD(\"ClientFile\")",
         "         ENDIF",
         "      ENDIF",
         "   ENDIF",
